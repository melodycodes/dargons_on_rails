<h1>Create a New Character!</h1>
<h2>Choose your class</h2>
<div class="row">
  <div class="col-md-12">
    <%= bootstrap_form_for(@character, url: wizard_path, method: :put) do |f| %>
      <%= render 'shared/error_messages' %>
      <%= f.select :character_class, options_for_select(@character_classes) %>
      <%= f.primary "Continue" %>
    <% end %>
  </div>
</div>
