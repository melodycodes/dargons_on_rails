<h1>Create a New Character!</h1>
<h2>Let's get started...</h2>
<div class="row">
  <div class="col-md-12">
    <%= bootstrap_form_for(@character) do |f| %>
      <%= render 'shared/error_messages' %>
      <%= f.text_field :name%>
      <%= f.select :level, options_for_select(1..10), label: "Starting Level"%>
      <%= f.primary "Continue" %>
    <% end %>
  </div>
</div>
